    <TopicName></TopicName>
    <SubscriptionName></SubscriptionName>
  </Messaging>
  <Token> <!-- Token informatation -->
    <Issuer></Issuer>
    <Audience></Audience>
    <SecurityKey>GreenHouse_IOT_Solution</SecurityKey>
  </Token>
  <Logger> <!-- Provide Service bus endpoint -->
    <BrokerConnection></BrokerConnection>
    <SolutionName>Greenhouse</SolutionName>
  </Logger>
  <HangFire>
    <Enabled></Enabled> <!-- Values : True/False -->
