      <SettingType>LiveData</SettingType>
      <Host></Host>
      <Vhost></Vhost>
      <User></User>
      <Password></Password>
      <Port></Port>
      <IsSecure>1</IsSecure>
      <Url></Url>
    </IOTConnectSetting>
    <IOTConnectSetting>
      <SettingType>UIAlert</SettingType>
      <Host></Host>
      <Vhost></Vhost>
      <User></User>
      <Password></Password>
      <Port></Port>
      <IsSecure>1</IsSecure>
      <Url></Url>
    </IOTConnectSetting>
  </IOTConnectSettings>
  <!--SQL server Database connection string -->
  <ConnectionString></ConnectionString>
